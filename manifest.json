{
	"name": "Clear Cache",
	"version": "1.1",
	"description": "A true one-click cache clearing button for web developers. That's it.",
	"manifest_version": 2,
	"permissions": [
		"browsingData"
	],
	"browser_action": {
		"default_icon": "icon-32.png",
		"default_title": "Clear Cache",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": ["scripts.js"],
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	}
}
